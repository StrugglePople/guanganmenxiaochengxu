/* components/member-card/member-card.wxss */
.member-body{
    position:relative;
    padding:32rpx 0 24rpx 130rpx;
}
.member-body .name{
    position:absolute;
    left:0;
    top:30rpx;
    width:100rpx;
    height:100rpx;
    line-height:100rpx;
    text-align:center;
    background:#7B99F3;
    color:#fff;
    border-radius:100rpx;
    font-size:24rpx;
}
.member-body .h4 {
    font-size:36rpx;
    line-height:50rpx;
    padding-bottom:10rpx;
}
.member-body .h4 .samll {
    font-size:26rpx;
    color:#a5a5a5;
    line-height:50rpx;
    margin-left:10rpx;
}
.member-body .p {
    font-size:26rpx;
    line-height:44rpx;

}
.member-body .p i {
    vertical-align:top;
    margin-top:8rpx;
    margin-right:10rpx;
}
.i-phone-sm, .i-user-sm {
    display:inline-block;
    vertical-align:middle;
    width:28rpx;
    height:28rpx;
}
.member-body .i-user-sm{
    background: url(data:image/svg+xml;base64,) no-repeat center center;
    background-size: auto 24rpx;
}
.member-body .i-phone-sm{
    background: url(data:image/svg+xml;base64,) no-repeat center center;
    background-size: auto 24rpx;
}