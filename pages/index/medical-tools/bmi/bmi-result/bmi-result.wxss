/* pages/index/medical-tools/bmi/bmi-result/bmi-result.wxss */

  .bmi-img-result-wrapper {
    height: 90px;
    width: 316px;
    margin: 0 auto;
    position: relative;
  }

  .bmi-img-result-wrapper .point {
    background: url("data:image/png;base64,") no-repeat;
    background-size: 16px 20px;
    width: 16px;
    height: 20px;
    position: absolute;
    top:0;
    left: 0px;
  }
  .bmi-img-result-wrapper .img-result-bar {
    background: url("data:image/png;base64,") no-repeat;
    background-size: 300px 51px;
    width:300px;
    height:51px;
    position: absolute;
    top: 20px;
    left: 8px;
  }  

  .result-type{
    color: #c6a494;
    font-size: 18px
  }
  .sort-type{
    color: #b1b1b1;
    font-size: 15px; 
  }


