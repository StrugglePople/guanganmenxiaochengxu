/* pages/customer/index/index.wxss */
.mainView{
  position: relative;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}
.chat-kf-wrap {
  /* position: absolute;
  height: 670rpx;
  width: 100%;
  overflow-y: auto; */
  padding-bottom: 30rpx;
}
.chat-kf {
  padding: 0 20rpx;
}
.chat-kf .time {
  width: 100rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin: 40rpx auto 0  auto;
  text-align: center;
  background: #DAD8D8;
  border-radius: 8rpx;
  font-size: 24rpx;
  color: #fff;
}
.chat-kf .other-side, .chat-kf .myself{
  margin-top: 32rpx;
}
.chat-kf .other-side .tx {
  float: left;
}
.chat-kf .other-side .tx image {
  display: block;
  width: 80rpx;
  height: 80rpx;
  margin-right: 30rpx;
}
.chat-kf .other-side .txt {
  position: relative;
  float: left;
  width: 488rpx;
  background: #FFFFFF;
  border-radius: 20rpx;
}
.chat-kf .other-side .txt::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  border: 1px solid #E5E5E5;
  -webkit-box-sizing: border-box;
  border-radius: 32rpx;
  box-sizing: border-box;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: left top;
  transform-origin: left top;
  pointer-events: none;
}
.chat-kf .other-side .txt .li {
  position: relative;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
  padding-right: 34rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 32rpx;
}
.chat-kf .other-side .txt .arrow+.li {
  border-top-left-radius:20rpx;
  border-top-right-radius:20rpx;
}
.chat-kf .other-side .txt .li:last-child {
  border-bottom-left-radius:20rpx;
  border-bottom-right-radius:20rpx;
}
.chat-kf .other-side .txt .li::before {
  position: absolute;
  bottom: 0;
  left: 0;
  content: ' ';
  width: 100%;
  height: 1px;
  border-bottom: 1px solid #E5E5E5;
  pointer-events: none;
}
.chat-kf .other-side .txt .li:last-child::before {
  border-bottom: none;
}
@media only screen and (-webkit-min-device-pixel-ratio: 2) {
    .chat-kf .other-side .txt .li::before{
        -webkit-transform: scaleY(0.5);
        transform: scaleY(0.5);
    }
}
@media only screen and (-webkit-min-device-pixel-ratio: 3) {
    .chat-kf .other-side .txt .li::before{
        -webkit-transform: scaleY(0.33);
            transform: scaleY(0.33);
    }
}
.chat-kf .other-side .txt .li .a {
  font-size: 32rpx;
  color: #111;
}
.chat-kf .other-side .txt .li image {
  display: block;
  width: 16rpx;
  height: 26rpx;
  margin-top: 27rpx;
}
.chat-kf .other-side .txt .li .i-item-angle {
  position: absolute;
  right: 20rpx;
  top: 0;
  bottom: 0;
  height: auto;
  margin-top: 0;
}
.chat-kf .other-side .txt .arrow {
  position: absolute;
  display: block;
  width: 12rpx;
  height: 18rpx;
  left: -10rpx;
  top: 33rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  z-index: 8787;
}
.chat-kf .myself .txt {
  position: relative;
  float: right;
  max-width: 500rpx;
  padding: 20rpx 24rpx;
  color: #fff;
  font-size: 32rpx;
  /* background-image: linear-gradient(-90deg, #53DB92 0%, #28CCC1 100%); */
  border-radius: 16rpx;
  background-color: #B79153;
}
.chat-kf .myself .txt .arrow {
  position: absolute;
  display: block;
  /* width: 12rpx; */
  /* height: 20rpx; */
  right: -12rpx;
  top: 30rpx;
  border-left: 12rpx solid #B79153;
  border-top: 10rpx solid #fff;
  border-bottom: 10rpx solid #fff;
}
.chat-kf .myself .tx {
  float: right;
}
.chat-kf .myself .tx image {
  display: block;
  width: 80rpx;
  height: 80rpx;
  margin-left: 30rpx;
}
.ft-kf {
  position: absolute;
  width: 100%;
  height: 100rpx;
  bottom: 0;
  left: 0;
  background: #fff;
}
.ft-kf::before {
  position: absolute;
  top: 0;
  left: 0;
  content: ' ';
  width: 100%;
  height: 1px;
  border-top: 1px solid #dadada;
}
.ft-kf .voice {
  display: block;
  float: left;
  width: 60rpx;
  height: 62rpx;
  margin: 20rpx 20rpx 0 32rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.ft-kf .add {
  display: block;
  float: left;
  width: 60rpx;
  height: 62rpx;
  margin-top: 20rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.ft-kf .bq {
  display: block;
  float: left;
  width: 60rpx;
  height: 62rpx;
  margin-top: 20rpx;
  margin-right: 18rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.ft-kf .input-text {
  position: relative;
  /* float: left; */
  width: 450rpx;
  height: 72rpx;
  min-height: 72rpx;
  margin: 14rpx 18rpx 0 0;
  line-height: 72rpx;
  background: #FDFCFC;
  border-radius: 10rpx;
  font-size: 32rpx;
}
.ft-kf .input-text::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  border: 1px solid #d2d2d2;
  -webkit-box-sizing: border-box;
  border-radius: 16rpx;
  box-sizing: border-box;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(0.5);
  transform: scale(0.5);
  -webkit-transform-origin: left top;
  transform-origin: left top;
}
.ft-kf .input-text input {
  position: relative;
  /* float: left; */
  /* width: 370rpx; */
  margin-left: 12rpx;
  margin-top: 10rpx;
  font-size: 32rpx;
  border: none;
  z-index: 879;
  height: 54rpx;
  padding-top: 0;
}
.ft-kf .input-text i {
  display: block;
  float: left;
  width: 28rpx;
  height: 36rpx;
  margin: 18rpx 0 0 20rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.clearfix::after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.clearfix::before,
.clearfix::after {
  display: table;
  content: "";
  line-height: 0;
}

.banner-kf,
.banner-kf image {
  position: relative;
  display: block;
  width: 100%;
  height: 160rpx;
}
.background {
  position: absolute;
  display: block;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 5rpx
}