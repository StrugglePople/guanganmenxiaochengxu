/* 持卡人名片显示start */
.am-panel{
  margin: 40rpx 24rpx;
  background: #FFFFFF;
  box-shadow: 0 12rpx 36rpx 0 rgba(0,0,0,0.10);
  border-radius: 28rpx;
}
.am-panel.first{
  margin-top: 0;
}
.am-panel-per{
  padding: 0 20rpx;
}
.visit-per-list .am-panel-per{
  display: inline-block;
  width: 660rpx;
}
.am-panel-per .bd{
  position: relative;
  padding: 32rpx 0 24rpx 130rpx;
}
.am-panel-per .name{
  position: absolute;
  left: 0;
  top: 30rpx;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background: #7B99F3;
  color: #fff;
  border-radius: 100rpx;
  font-size: 34rpx;
}
.am-panel-per .name-danger{
  background: #FF9571;
}
.am-panel-per .name-success{
  background: #57CFA1;
}
.am-panel-per .h4{
  font-size: 36rpx;
  line-height: 50rpx;
  padding-bottom: 10rpx;
}
.am-panel-per .samll{
  font-size: 26rpx;
  color: #a5a5a5;
  line-height: 50rpx;
  margin-left: 10rpx;
}
.am-panel-per .p{
  font-size: 26rpx;
  line-height: 44rpx;
}
.am-panel-per .p i{
  vertical-align: top;
  margin-top: 8rpx;
  margin-right: 10rpx;
}
.am-panel-per .ft{
  position: relative;
  padding: 12rpx 0;
}
.am-panel-per .ft:before{
    content: "";
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    border-top: 1px dashed #E5E5E5;
    -webkit-transform-origin: left bottom;
    transform-origin: left bottom;
}
.am-panel-per .ft-qr-code{
  text-align: center;
  padding: 20rpx 0;
}
.am-panel-per .ft-qr-code image{
  width:256rpx;
  height:60rpx;
  margin: 0 auto;
}
.i-phone-sm, .i-user-sm{
  display: inline-block;
  vertical-align: middle;
  width: 28rpx;
  height: 28rpx;
}
.i-phone-sm{
  background: url(data:image/svg+xml;base64,) no-repeat center center;
  background-size: auto 24rpx; 
}
.i-user-sm{
  background: url(data:image/svg+xml;base64,) no-repeat center center;
    background-size: auto 24rpx; 
}

/* 持卡人名片显示end */
/* 公用背景 */
.common-bg{
  width: 750rpx;
  height: 190rpx;
}
.common-bg.select-crard-bg{
  position: absolute;
  top: 0;
  left: 0;
  
}


/* 记录类列表start */
.rgrecond-item {
  position: relative;
  padding: 20rpx 24rpx;
}
.rgrecond-item .h5-lg {
  font-size: 36rpx;
  font-weight: 500;
}
.rgrecond-item .h5 {
  font-size: 32rpx;
}
.rgrecond-item .p {
  color: #888;
  font-size: 24rpx;
}

.rgrecond-item::before {
  content: "";
  position: absolute;
  z-index: 10;
  left: 24rpx;
  right: 0;
  top: 0;
  border-top: 1rpx solid #e5e5e5;
  pointer-events: none;
}

.rgrecond-item-list .rgrecond-item:first-child:before {
  display: none;
}

.rgrecond-item .txt-rsmall {
  font-size: 24rpx;
  margin-top: 48rpx;
}
/* 记录类列表end */
/* 查报告单 */

.text-panel {
  background: #fff;
  padding: 0 24rpx;
}

.text-panel .am-flex-item {
  padding-top: 20rpx;
}

.text-panel .h4 {
  font-size: 40rpx;
}

.text-panel .small {
  color: #a5a5a5;
  font-size: 24rpx;
}

.text-panel-hd, .text-panel-bd {
  padding-bottom: 20rpx;
}

/* 查报告单 end */
/* 检验详情列表 start */

.am-tb {
  background: #fff;
}

.thead .tr {
  margin: 0;
  padding: 0 24rpx;
}

.tr {
  position: relative;
  display: flex;
  justify-content: space-between;
  margin: 0 24rpx;
}

.tr::before {
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  border-bottom: 1rpx solid #e5e5e5;
}

.tfoot .td {
  color: #888;
  font-size: 32rpx;
}

.am-tb .tr:last-child::before {
  border-bottom: none;
}

.am-tb .thead .tr::before {
  border-bottom: 1rpx solid #e5e5e5;
}

.th {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  color: #888;
}

.td {
  min-height: 88rpx;
  width: 100%;
  line-height: 52rpx;
  color: #111;
  padding: 18rpx 0;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.td-right {
  text-align: right;
}

.td-center {
  text-align: center;
}

.unit-td {
  display: inline-block;
  vertical-align: middle;
  font-size: 26rpx;
  color: #111;
  margin-right: 2rpx;
}

.num-td {
  display: inline-block;
  vertical-align: middle;
  font-size: 32rpx;
  color: #111;
}

.num-td-lg {
  display: inline-block;
  vertical-align: middle;
  font-size: 40rpx;
  color: #111;
}

/* 检验详情列表 end*/