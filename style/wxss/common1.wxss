.wx-row{
  display: -webkit-box;
  display: -webkit-flex;
  -webkit-box-orient: horizontal;
  -webkit-flex-flow: row nowrap;
  flex-flow: row nowrap;
}
.wx-col, .wx-row li{
  -webkit-box-flex: auto;
  -webkit-flex: auto;
  flex: auto;
  position: relative;
}
.per-menu-list {
  -webkit-box-orient: horizontal;
  padding: 0 10rpx;
}

.per-menu-list li {
  position: relative;
  padding: 28rpx 0 20rpx 0;
  text-align: center;
}
.per-menu-list image{
  width: 70rpx;
  height: 70rpx;
}
.per-menu-list .txt{
  display: block;
  margin-top: 10rpx;
}
/* 列表下划线 */
.item-bot-line{
  position:relative;
  padding: 20rpx 24rpx;
}
.item-bot-line:before{
    content: "";
    position: absolute;
    z-index: 10;
    left: 24rpx;
    right: 0;
    bottom: 0;
    height: 1px;
    background: #EEEEEE;
}
.item-bot-line.full:before{
    left: 0;
}


