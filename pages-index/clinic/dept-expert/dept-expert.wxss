/* pages/index/clinic/dept-expert/dept-expert.wxss */
.search-rcont {
  right: 0
}
.search-lside {
  width: 280rpx;
}
.search-rcont {
  left: 280rpx;
}
.img-search{
  position: absolute;
  top: 28rpx;
  left: 28rpx;
  width: 44rpx;
  height: 44rpx;
}
.search-bar{
  background: #fff;
  height: 100rpx;
}
.search-bar input{
  width: 100%;
  height: 100rpx;
  font-size: 32rpx;
  line-height: 100rpx;
  padding: 0 28rpx 0 92rpx;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
.search-lside{
  position: absolute;
  top: 0;
  bottom: 0;
  overflow: auto;
  width: 230rpx;
  background: #F9F9F9;
  box-shadow: inset 0 0 0 0 #DADADA;
}
.search-rcont{
  position: absolute;
  top: 0;
  left: 230rpx;
  bottom: 0;
  overflow: auto;
}
.index-list .index{
  line-height: 54rpx;
  color: #a5a5a5;
  padding: 0 28rpx;
}
.index-list li{
  display: block;
  line-height: 88rpx;
  padding: 0 20rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.index-list li.active{
  background:#FFFFFF;
  color:#B79153;
  border-left:6rpx solid #B79153;
  padding-left:14rpx;
}
.index-panel{
  padding: 20rpx;
}
.baner-panel-link{
  min-height: 80rpx;
  position: relative;
  padding: 20rpx 56rpx 20rpx 20rpx;
  color: #FF513C;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100%; 
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
.baner-panel-link .i-angle-r-danger{
  position: absolute;
  top: 50%;
  right: 28rpx;
  margin-top: -14rpx;
}
.i-angle-r-danger{
  display: inline-block;
  vertical-align: top;
  width: 28rpx;
  height: 28rpx;
  margin-top: 11rpx;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: auto 26rpx;
}

.img-face-lg{
  width: 92rpx;
  height: 92rpx;
}
.img-face-lg-primary{
  background: #FCF6EA;
  line-height: 92rpx;
  text-align: center;
  font-size: 32rpx;
  color: #AE700D;
  border-radius: 92rpx;
}
.span-auxiliary-sm{
  font-size: 24rpx;
  padding: 5rpx 18rpx;
  border-radius: 4rpx;
}

.item-img-multitxt-list{
  background: #fff;
}
.item-img-multitxt{
  min-height: 128rpx;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  position: relative;
  padding: 20rpx 24rpx 20rpx 136rpx;
}

.item-img-multitxt:before{
    content: "";
    position: absolute;
    z-index: 10;
    left: 24rpx;
    right: 0;
    top: 0;
    height: 1px;
    background: #E5E5E5;
}
.item-img-multitxt-list .item-img-multitxt:first-child:before{
  display: none;
}
@media only screen and (-webkit-min-device-pixel-ratio: 2) {
    .item-img-multitxt:before{
        -webkit-transform: scaleY(0.5);
        transform: scaleY(0.5);
    }
}
@media only screen and (-webkit-min-device-pixel-ratio: 3) {
    .item-img-multitxt:before{
        -webkit-transform: scaleY(0.33);
            transform: scaleY(0.33);
    }
}
.item-img-multitxt .img-face-lg{
  position: absolute;
  left: 24rpx;
  top: 20rpx;
}
.item-img-multitxt .h5{
  font-size: 32rpx;
  font-weight: 500;
}
.item-img-multitxt .h5-lg{
  font-size: 36rpx;
  font-weight: 500;
}
.item-img-multitxt text{
  margin-right: 10rpx;
}
.item-img-multitxt .am-flex-after text{
  margin-right: 0;
  margin-left: 10rpx;
}
.item-img-multitxt .small{
  font-size: 24rpx;
  color: #a5a5a5;
}
.item-img-multitxt .p{
  font-size: 24rpx;
  color: #a5a5a5;
  margin-top: 6rpx;
}
.item-img-multitxt .p.nowrap{
  overflow: hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}

.span-auxiliary-gray{
  background: #DAD8D8;
  color: #fff;
}